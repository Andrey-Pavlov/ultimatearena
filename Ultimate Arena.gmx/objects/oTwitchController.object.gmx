var data = ds_map_find_value(async_load, "buffer");
var str  = buffer_read(data, buffer_string);

trace(str);

if (str == "PING .tmi.twitch.tv")
{
    irc_send_message("PING");
}
if (string_extract(str," ",1) == "NOTICE")
{
    ui_show_popup("Twitch Connection Unsuccessful");
    global.TWITCHENABLED = 0;
}

if(string_extract(str," ",1) == "PRIVMSG")
{
    se = string_extract((string_upper(str))," ",3);
    if(se == ":!VOTE")
    {
        sendVote(voteType,string_extract(str," ",4),getTwitchID(str));
        se = "";
    }
    
    se = string_parse(str," ",1);
    
    asd = string_rtrim(string_upper(ds_list_find_value(se, 3)));
    if(asd == pac)
    {
        
        getPoints(getTwitchID(str));
        
        
    }

    ds_list_destroy(se);
    
}


